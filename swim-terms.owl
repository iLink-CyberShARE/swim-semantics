    <!-- http://purl.org/swim/terms#Adjective -->

    <owl:Class rdf:about="http://purl.org/swim/terms#Adjective">
        <rdfs:comment>A word or phrase naming or describing an attribute.</rdfs:comment>
        <rdfs:label>adjective</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/swim/terms#Citizen -->

    <owl:Class rdf:about="http://purl.org/swim/terms#Citizen">
        <rdfs:subClassOf rdf:resource="http://purl.org/swim/terms#GeneralPublic"/>
        <rdfs:comment>An inhabitant of a particular town or city.</rdfs:comment>
        <rdfs:label>citizen</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/swim/terms#Explanation -->

    <owl:Class rdf:about="http://purl.org/swim/terms#Explanation">
        <rdfs:comment>A statement or account that makes the values a model element clear.</rdfs:comment>
        <rdfs:label>explanation</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/swim/terms#Farmer -->

    <owl:Class rdf:about="http://purl.org/swim/terms#Farmer">
        <rdfs:subClassOf rdf:resource="http://purl.org/swim/terms#GeneralPublic"/>
        <rdfs:comment>A person who owns or manages a farm.</rdfs:comment>
        <rdfs:label>farmer</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/swim/terms#FocusArea -->

    <owl:Class rdf:about="http://purl.org/swim/terms#FocusArea">
        <rdfs:comment>An area of focus derived from the types of water consumption for the SWIM framework. I.e. urban, agriculture, recreation</rdfs:comment>
        <rdfs:label>focus area</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/swim/terms#GeneralPublic -->

    <owl:Class rdf:about="http://purl.org/swim/terms#GeneralPublic">
        <rdfs:subClassOf rdf:resource="http://purl.org/swim/terms#UserType"/>
        <rdfs:comment>Refers to any type of person or user.</rdfs:comment>
        <rdfs:label>general public</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/swim/terms#Input -->

    <owl:Class rdf:about="http://purl.org/swim/terms#Input">
        <rdfs:subClassOf rdf:resource="http://purl.org/swim/terms#ModelElement"/>
        <rdfs:comment>An input element to the model. I.e Model Parameter</rdfs:comment>
        <rdfs:label>input</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/swim/terms#ModelElement -->

    <owl:Class rdf:about="http://purl.org/swim/terms#ModelElement">
        <rdfs:comment>An attribute of a scientific model that can be specified as an input, set, scenario, output or setting.</rdfs:comment>
        <rdfs:label>model element</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/swim/terms#Narrative -->

    <owl:Class rdf:about="http://purl.org/swim/terms#Narrative">
        <rdfs:comment>A  written account that aids in the understanding of given values of a model element.</rdfs:comment>
        <rdfs:label>narrative</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/swim/terms#ODMClass -->

    <owl:Class rdf:about="http://purl.org/swim/terms#ODMClass">
        <rdfs:comment>A java class that defines the mapping between a json document stored in mongo and an instance of the java class.</rdfs:comment>
        <rdfs:label>object document mapping class</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/swim/terms#Output -->

    <owl:Class rdf:about="http://purl.org/swim/terms#Output">
        <rdfs:subClassOf rdf:resource="http://purl.org/swim/terms#ModelElement"/>
        <rdfs:comment>An element which values are generated from a model run. I.e. model output variable.</rdfs:comment>
        <rdfs:label>output</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/swim/terms#PolicyMaker -->

    <owl:Class rdf:about="http://purl.org/swim/terms#PolicyMaker">
        <rdfs:subClassOf rdf:resource="http://purl.org/swim/terms#GeneralPublic"/>
        <rdfs:comment>A user with the role of creating institutional policy.</rdfs:comment>
        <rdfs:label>policy maker</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/swim/terms#Scenario -->

    <owl:Class rdf:about="http://purl.org/swim/terms#Scenario">
        <rdfs:subClassOf rdf:resource="http://purl.org/swim/terms#ModelElement"/>
        <rdfs:comment>A predefined parameterized situation to reflect a potential projection.</rdfs:comment>
        <rdfs:comment>scenario</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/swim/terms#Scientist -->

    <owl:Class rdf:about="http://purl.org/swim/terms#Scientist">
        <rdfs:subClassOf rdf:resource="http://purl.org/swim/terms#GeneralPublic"/>
        <rdfs:comment>A person who is studying or has expert knowledge of one or more of the natural or physical sciences.</rdfs:comment>
        <rdfs:label>scientist</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/swim/terms#Set -->

    <owl:Class rdf:about="http://purl.org/swim/terms#Set">
        <rdfs:subClassOf rdf:resource="http://purl.org/swim/terms#ModelElement"/>
        <rdfs:comment>A group or collection of things that belong together. I.e. Crops</rdfs:comment>
        <rdfs:label>set</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/swim/terms#Setting -->

    <owl:Class rdf:about="http://purl.org/swim/terms#Setting">
        <rdfs:subClassOf rdf:resource="http://purl.org/swim/terms#ModelElement"/>
        <rdfs:comment>Configuration setting of a target model. I.e. Resolution</rdfs:comment>
        <rdfs:label>setting</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/swim/terms#Template -->

    <owl:Class rdf:about="http://purl.org/swim/terms#Template">
        <rdfs:comment>A preset format that serves as a skeleton for the generation of an element narrative.</rdfs:comment>
        <rdfs:comment>template</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/swim/terms#TemplateCatalog -->

    <owl:Class rdf:about="http://purl.org/swim/terms#TemplateCatalog">
        <rdfs:comment>A catalog or index that holds references to template inventories on the SWIM database.</rdfs:comment>
        <rdfs:label>template catalog</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/swim/terms#TemplateInventory -->

    <owl:Class rdf:about="http://purl.org/swim/terms#TemplateInventory">
        <rdfs:comment>An inventory of templates, adjectives and explanations that used for the generation of a narrative to describe a model element.</rdfs:comment>
        <rdfs:label>template inventory</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/swim/terms#UserType -->

    <owl:Class rdf:about="http://purl.org/swim/terms#UserType">
        <rdfs:comment>A type or role that a user belongs to.</rdfs:comment>
        <rdfs:label>user type</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/swim/terms#WaterAdministrator -->

    <owl:Class rdf:about="http://purl.org/swim/terms#WaterAdministrator">
        <rdfs:subClassOf rdf:resource="http://purl.org/swim/terms#GeneralPublic"/>
        <rdfs:comment>A person that manages use and distribution of a water sources.</rdfs:comment>
        <rdfs:label>water administrator</rdfs:label>
    </owl:Class>
